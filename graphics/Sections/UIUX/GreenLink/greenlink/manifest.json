{
  "manifest_version": 2,
  "name": "GreenLink",
  "version": "1.0",
  "description": "Cute eco-friendly messages when you open a tab!",
  "permissions": [
   "tabs",
   "activeTab"
  ],

  "chrome_url_overrides": {
   "newtab": "popup.html"
  },

  "browser_action": {
    "default_title": "GreenLink",
    "default_icon": {
      "192": "Images/greenlink_192.png",
      "512": "Images/greenlink_512.png"
    }
  }
}
